pragma solidity ^0.8.0;

contract Ownable {
    address public Rogelio;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    constructor() {
        Rogelio= msg.sender;
    }
    modifier onlyOwner() {
        require(msg.sender == Rogelio, "Ownable: caller is not the Rogelio");
        _;
    }
    function transferOwnership(address newOwner) public onlyOwner {
        emit OwnershipTransferred(Rogelio, newOwner);
        Rogelio= newOwner;
    }
}
